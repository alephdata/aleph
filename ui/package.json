    "test": "craco test",
    "eject": "craco eject",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,css,scss}'",
    "translate": "npm run messages && npm run compile-translations && npm run concat-translations",
