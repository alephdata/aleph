<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for collection in collections -%}
    <url>
        <loc>{{ collection.url | safe }}</loc>
        <lastmod>{{ collection.updated_at }}</lastmod>
        <changefreq>monthly</changefreq>
    </url>
    {%- endfor %}
</urlset>
